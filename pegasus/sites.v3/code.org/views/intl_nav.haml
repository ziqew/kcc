#about_nav
  %span.about_list
    -current_url = request.path_info
    -{'About the Program'=>'/international',
      'International Partner Application'=> '/international/apply'}.each_pair do |label,url|
      - if current_url == url
        %span.about_list_item_selected= label
      -else
        %a.about_list_item{:href=>url}= label
      %hr.narrow_hr

